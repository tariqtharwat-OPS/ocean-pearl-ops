{
    "name": "ops-functions",
    "version": "2.0.0",
    "description": "Ocean Pearl OPS V2 Cloud Functions",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log",
        "seed": "tsx src/seed.ts",
        "test:t1": "tsx tests/testT1.ts",
        "test:t2": "tsx tests/testT2.ts"
    },
    "engines": {
        "node": "20"
    },
    "main": "lib/index.js",
    "dependencies": {
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^5.1.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}